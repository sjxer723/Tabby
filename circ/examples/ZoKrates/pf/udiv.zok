def main(private u8 x, private u8 y) -> bool:
    u8 q = x / y
    u8 r = x % y
    bool c0 = 0 <= r
    bool c1 = r < y
    bool c2 = x == q*y + r

    u8 q2 = x / 17
    u8 r2 = x % 17
    bool c3 = 0 <= r2
    bool c4 = r2 < 17
    bool c5 = x == q2*17 + r2

    return c0 && c1 && c2 && c3 && c4 && c5
